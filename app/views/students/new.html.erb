<h2>Student Form</h2>

<%= form_tag students_path do %>

    <label> Student first name: </label><br>
    <%= text_field_tag name='student[first_name]' %><br>

    <lable> Student last name: </label><br>
    <%= text_field_tag name='student[last_name]' %><br>

    <%= submit_tag "Submit Student" %>
        
<% end %>